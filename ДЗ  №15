// Домашнее задание №15
Console.WriteLine("Введите число");
int Number = Convert.ToInt32(Console.ReadLine());
int a = Convert.ToInt32 (0);
int b = Convert.ToInt32 (7);
int c = Convert.ToInt32 (23);

if (Number % b == a)
{
    Console.WriteLine ("Кратно 7");
}
else if (Number % c == a)
{
    Console.WriteLine ("кратно 23");
}
